centroids.df = readRDS("centroids.df.1-1900.rds")
split.df = lapply(unique(centroids.df$ident), function(x){
subset = subset(centroids.df, ident == x)
df = data.frame(x = subset[1,"X"],
y = subset[1,"Y"],
xend = subset[nrow(subset),"X"],
yend = subset[nrow(subset),"Y"],
ident = x)
return(df)
})
split.df = do.call(rbind, split.df)
library(ggplot2)
ggplot(centroids.df, aes(x = X, y = Y, group = ident))+
geom_point(aes(color = frame)) +
# background_image(getFrame(video_retina_mask, start_frame + frame - 1)) +
scale_x_continuous(limits = c(0, 800), expand = c(0,0))+
scale_y_reverse(limits = c(680, 0), expand = c(0,0))+
scale_color_gradientn(colours = rainbow(10))+
theme_classic()+
geom_line()+
geom_segment(aes(x = x,
y = y,
xend = xend,
yend = yend),
# color = as.character(ident)),
arrow = arrow(length = unit(0.3, "cm")),
data = split.df,
alpha = 1)
